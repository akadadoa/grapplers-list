# Copy this file to .env.local and fill in your values

# Mapbox - Public token (used client-side by the map)
NEXT_PUBLIC_MAPBOX_TOKEN=pk.xxx

# Mapbox - Secret token (used server-side for geocoding)
MAPBOX_SECRET_TOKEN=sk.xxx

# Random secret to guard the /api/scrape endpoint
SCRAPE_SECRET=change-me-to-something-random

# SQLite database path (relative to project root)
DATABASE_URL="file:./dev.db"
